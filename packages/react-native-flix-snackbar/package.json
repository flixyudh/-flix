        "infile": "CHANGELOG.md"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flixyudh/flix.git"
  },
  "homepage": "https://github.com/flixyudh/flix/tree/master/packages/react-native-flix-snackbar",
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "author": "Flix <flixy121@gmail.com> (https://github.com/flixyudh)",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@types/react-native": "0.70.0"
  }
