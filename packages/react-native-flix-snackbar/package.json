{
  "name": "@flix-react-native/snackbar",
  "version": "1.0.3",
  "description": "Easy usage of snackbar without ref",
  "main": "dist/index.js",
  "react-native": "./dist/index",
  "module": "dist/index",
  "types": "dist/index.d.ts",
  "source": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
